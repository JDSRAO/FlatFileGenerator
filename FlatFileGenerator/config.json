{
  "fileName": "test.csv",
  "showRowNumber": true,
  "seperator": ",",
  "rows": 1000,
  "columns": [
    {
      "name": "Row id",
      "type": "guid"
    },
    {
      "name": "Created On",
      "type": "date",
      "config": {
        "format": "dd-MM-yyyy"
      }
    },
    {
      "name": "Today date",
      "type": "default",
      "config": {
        "defaultValue": "kjds"
      }
    },
    {
      "name": "First Name",
      "type": "string",
      "config": {
        "prefix": "KJDS"
      }
    },
    {
      "name": "Last Name",
      "type": "string",
      "config": {
        "length": "2|5"
      }
    },
    {
      "name": "SortCode",
      "type": "int",
      "config": {
        "prefix": "TEST-",
        "suffix": "SERIES"
      }
    },
    {
      "name": "Account Number",
      "type": "int",
      "config": {
        "prefix": "SAS-",
        "increment": true,
        "min": 1000,
        "interval": 10
      }
    },
    {
      "name": "Email",
      "type": "email"
    },
    {
      "name": "Amount",
      "type": "decimal",
      "config": {
        "decimalPart": 4
      }
    },
    {
      "name": "IsValid",
      "type": "bool"
    },
    {
      "name": "CustomValues",
      "type": "list",
      "config": {
        "items": [ "kjds", 1, "bool", "any" ]
      }
    }
  ]
}
